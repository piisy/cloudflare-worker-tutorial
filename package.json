{
  "name": "little-eleven-api",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:local": "wrangler dev",
    "start:remote": "wrangler dev --remote",
    "deploy": "wrangler deploy",
    "generate:openapi": "curl http://localhost:8787/api/doc > ./openapi.json",
    "generate:sql": "drizzle-kit generate",
    "login": "wrangler login",
    "d1:create": "wrangler d1 create eleven",
    "d1:execute:local": "wrangler d1 execute eleven --file ./drizzle/0000_faithful_sleeper.sql",
    "d1:execute:remote": "wrangler d1 execute eleven --remote --file ./drizzle/0000_faithful_sleeper.sql",
    "d1:export:remote": "wrangler d1 export eleven --remote --output=./schema.sql",
    "tsc-check": "tsc --noEmit",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.16.4",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.6",
    "minimatch": "^10.0.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/jest": "^29.5.14",
    "better-sqlite3": "^11.5.0",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3",
    "wrangler": "^3.83.0"
  }
}
